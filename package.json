{
    "name": "cool-boilerplate",
    "version": "0.0.1",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.babel",
        "build:client:dev": "cross-env NODE_ENV=development webpack --config config/webpack.dev.babel --color",
        "build:server:dev": "cross-env NODE_ENV=development webpack --config config/webpack.server.babel --color",
        "build:dev": "cross-env yarn run build:server:dev && yarn run build:client:dev",
        "build:client:production": "cross-env NODE_ENV=production webpack --config config/webpack.prod.babel --color -p",
        "build:server:production": "cross-env NODE_ENV=production webpack --config config/webpack.server.babel --color -p",
        "build:production": "cross-env yarn run build:server:production && yarn run build:client:production",
        "clean": "rm -rf build",
        "test": "cross-env eslint src",
        "deploy": "cross-env yarn run clean && yarn run build:production"
    },
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-react-transform": "^2.0.2",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-async-to-generator": "^6.8.0",
        "babel-plugin-transform-class-properties": "^6.10.2",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-object-assign": "^6.8.0",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-1": "^6.5.0",
        "babel-regenerator-runtime": "^6.5.0",
        "cross-env": "^4.0.0",
        "es6-promise": "^4.0.5",
        "eslint": "^3.19.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-react": "^7.0.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "flow-bin": "^0.48.0",
        "html-webpack-plugin": "^2.22.0",
        "mobx": "^3.1.9",
        "mobx-react": "^4.0.3",
        "mobx-react-devtools": "^4.2.10",
        "prettier-eslint-cli": "^4.0.1",
        "raw-loader": "^0.5.1",
        "react": "^15.5.4",
        "react-apollo": "^1.2.0",
        "react-dom": "^15.5.4",
        "react-hot-loader": "3.0.0-beta.6",
        "react-router": "3.0.5",
        "resolve-url-loader": "^2.0.2",
        "script-ext-html-webpack-plugin": "^1.6.2",
        "styled-components": "^2.1.0",
        "url-loader": "^0.5.7",
        "webpack": "^2.5.1",
        "webpack-dev-server": "^2.4.5"
    },
    "peerDependencies": {}
}
